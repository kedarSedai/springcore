<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.example.testsring.CollectionInjection.Employee" name="employee">
        <property name="address">
            <list>
                <value>"Sifal"</value>
                <value>"kalopul"</value>
                <value>"kapan"</value>
                <value>"kapan"</value>
            </list>
        </property>

        <property name="city">
            <set>
                <value>kathamandu</value>
                <value>kathamandu</value>
                <value>null</value>
                <value>null</value>
            </set>
        </property>

        <property name="college">
            <map>
                <entry key="1" value="Deerwalk"/>
                <entry key="2" value="NCIT"/>
            </map>
        </property>

    </bean>
</beans>